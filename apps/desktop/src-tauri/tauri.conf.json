{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Nekoshare",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:7783",
    "beforeDevCommand": "pnpm dev:w",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "Nekoshare Desktop",
        "label": "main",
        "minWidth": 1200,
        "minHeight": 700,
        "center": true,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
